{
    "version": 2,
    "builds": [
        {
            "src": "ineterpo/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "staticfiles",
                "zeroConfig": true
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "ineterpo/wsgi.app"
        }
    ],
    "env": {
        "PYTHONPATH": "/var/task",
        "DJANGO_SETTINGS_MODULE": "ineterpo.settings"
    }
}